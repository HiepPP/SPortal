@using SetPortal.Models
@model List<EGuide>


@if (Model.Count() != 0)
{
    foreach (var item in Model)
    {
        <div class="DivRegis">@item.ChuDeEGuide.TenChuDeEguide</div>
        <div class="DivA">@Html.DisplayFor(n => item.Contents)</div>
        <div class="Dowload"><a href="~/FileTaiLieuEGuild/@item.FileName">DownLoad</a></div><br><hr />
    }
}
else
{
    <p>Khong tim thay</p>
}

