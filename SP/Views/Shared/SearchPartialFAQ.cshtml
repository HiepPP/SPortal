@using SetPortal.Models
@model List<FAQ>


@if (Model.Count != 0)
{
    foreach (var item in Model)
    {
        <div class="DivRegis">@item.ChuDeFAQ.TenChuDeFAQ</div>
        <div class="DivQ"><span class="ChuQ">Q:</span>@Html.DisplayFor(n => item.Question)</div>
        <div class="DivA"><span class="ChuA">A:</span>@Html.DisplayFor(n => item.Answer)</div>
        <div class="Dowload"><a href="~/FileTaiLieuFAQ/@item.FileName">DownLoad</a></div><br><hr />
    }
}
else
{
    <p>Khong tim thay</p>
}
